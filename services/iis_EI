apiVersion: v1
kind: Service
metadata:
  name: iis_ei
  labels:
    app: iis_ei
spec:
  ports:
      name: http
      protocal: tcp
      port: 80
      targetPort: 9376
  externalIPs:
    - 192.168.218.50
---
apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app: iis_ei
  name: iis_ei
spec:
  replicas: 2
  selector:
    matchLabels:
      app: iis_ei
  template:
    metadata:
      labels:
        app: iis_ei
      name: iis_ei
    spec:
     containers:
      - name: iis_ei
        image: leandroscar/iis-windows-ltsc2019
     nodeSelector:
      kubernetes.io/os: windows
